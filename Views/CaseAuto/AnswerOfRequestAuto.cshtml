@{
    Layout = "_Layout";
    @using CaseAutoService.Model;
}

<table class="table">
   
    <tr>
        <td><strong>Name</strong></td>
        <td><strong>Manufacturer</strong></td>
        <td><strong>BodyType</strong></td>
        <td><strong>Price</strong></td>
        @foreach (var Auto in ViewBag.List)
        {
            <tr><td>@Auto.Name</td> <td>@Auto.Manufacturer</td> <td>@Auto.BodyType</td> <td>@Auto.Price руб.</td>  </tr>
        }
    </table>

